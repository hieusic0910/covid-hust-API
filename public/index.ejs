		<h1>Covid-hust API</h1>
		
		
	</section>

	

	<section class="snippet">
		<h3 class="title">End Points</h3>
		
		<div class="code">
			<div class="json">
				<br>
				&emsp;&emsp; /all<br>
				&emsp;&emsp; /countries<br>
				&emsp;&emsp; /countries/:query<br>
				&emsp;&emsp; /continents<br>
				&emsp;&emsp; /continents/:query<br>
				&emsp;&emsp; /historical<br>
				&emsp;&emsp; /historical/all<br>
				&emsp;&emsp; /historical/:query/:province?<br>
				&emsp;&emsp; /jhucsse<br>
				&emsp;&emsp; /jhucsse/counties/:county?<br>
			
			</div>
		</div>
		
	</section>

	

	
